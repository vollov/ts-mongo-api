{
  "name": "ts-tests",
  "version": "1.0.0",
  "description": "",
  "main": "dist/boot.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node --inspect=5858 -r ts-node/register ./ts/boot.ts",
    "start:watch": "nodemon",
    "test": "jest --detectOpenHandles --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bunyan": "^1.8.12",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "graphql": "^14.3.0",
    "graphql-server-express": "^1.4.0",
    "graphql-tools": "^4.0.4",
    "ioredis": "^4.9.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.9",
    "nodemon": "^1.19.0",
    "ts-node": "^8.1.1",
    "uuidv4": "^4.0.0"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.16.1",
    "@types/graphql": "^14.2.0",
    "@types/ioredis": "^4.0.10",
    "@types/jest": "^24.0.13",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mongoose": "^5.5.1",
    "@types/node": "^12.0.2",
    "@types/uuidv4": "^2.0.0",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "ts"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}